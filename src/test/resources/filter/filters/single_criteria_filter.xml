<?xml version="1.0" encoding="utf-8"?>
<!-- Simple single filter -->
<filter>
    <filters>
        <statuses>OPEN</statuses>
        <include>true</include>
    </filters>
    <filters>
        <severities>MAJOR</severities>
        <include>true</include>
    </filters>
    <filters>
        <components>MyComponent1</components>
        <include>true</include>
    </filters>
    <filters>
        <rules>MyRule1</rules>
        <include>true</include>
    </filters>
    <filters>
        <anew>true</anew>
        <include>true</include>
    </filters>
    <filters>
        <createdfrom>2012-12-13</createdfrom>
        <createdto>2013-12-14</createdto>
        <include>true</include>
    </filters>
</filter>
